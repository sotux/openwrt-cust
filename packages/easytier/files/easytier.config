config easytier
    option enabled '0'
    option easytierbin '/usr/bin/easytier-core'
    option webbin '/usr/bin/easytier-web'

config easytierweb
	option enabled '0'
    option db_path '/etc/easytier/et.db'
    option web_protocol 'udp'
    option web_port '8080'
    option api_port '8081'
    option weblog 'info'
